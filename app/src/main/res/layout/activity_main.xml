<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/textView2"
        android:layout_width="319dp"
        android:layout_height="136dp"
        android:fontFamily="@font/pettaya"
        android:text="Ramalan Shio dan Zodiak"
        android:textAlignment="center"
        android:textColor="@android:color/holo_purple"
        android:textSize="50sp"

        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.068" />

    <ImageButton
        android:id="@+id/bt_cari_zodiak"
        android:layout_width="157dp"
        android:layout_height="138dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="208dp"
        android:background="#00FFFFFF"
        android:onClick="bt_cari_zodiak"
        app:layout_constraintEnd_toStartOf="@+id/ramalan"
        app:layout_constraintHorizontal_bias="0.191"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7"
        app:srcCompat="@drawable/cekshio" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="159dp"
        android:layout_height="32dp"
        android:layout_marginStart="40dp"
        android:fontFamily="@font/popreg"
        android:text="Ramalan Zodiak"
        android:textAlignment="center"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/bt_cari_zodiak" />

    <ImageButton
        android:id="@+id/daftarshio"
        android:layout_width="141dp"
        android:layout_height="137dp"
        android:layout_marginTop="212dp"
        android:layout_marginEnd="30dp"
        android:background="#03FFFFFF"
        android:onClick="ramalan"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7"
        app:srcCompat="@drawable/ramalan1" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="143dp"
        android:layout_height="30dp"
        android:layout_marginEnd="30dp"
        android:fontFamily="@font/popreg"
        android:text="Ramalan Shio"
        android:textAlignment="center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/daftarshio" />

    <ImageButton
        android:id="@+id/ramalan"
        android:layout_width="141dp"
        android:layout_height="137dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="30dp"
        android:background="#03FFFFFF"
        android:onClick="ramalan"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7"
        app:srcCompat="@drawable/carishio" />

    <ImageButton
        android:id="@+id/bt_daftar_zodiak"
        android:layout_width="157dp"
        android:layout_height="138dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="16dp"
        android:background="#00FFFFFF"
        android:onClick="bt_cari_zodiak"
        app:layout_constraintEnd_toStartOf="@+id/ramalan"
        app:layout_constraintHorizontal_bias="0.254"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7"
        app:srcCompat="@drawable/carizodiak" />

    <EditText
        android:id="@+id/datedeparture"
        android:layout_width="175dp"
        android:layout_height="48dp"
        android:layout_marginTop="20dp"
        android:background="@color/teal_700"

        android:backgroundTint="#03913232"
        android:clickable="true"
        android:focusable="true"
        android:hint="Date"
        android:padding="12dp"
        android:textAlignment="center"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.529"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="161dp"
        android:layout_height="54dp"
        android:layout_marginTop="76dp"
        android:fontFamily="@font/popreg"
        android:text="Silahkan Pilih menu dibawah ini"
        android:textAlignment="center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="156dp"
        android:layout_height="27dp"
        android:layout_marginStart="44dp"
        android:text="Cari Zodiak"
        android:textAlignment="center"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/bt_daftar_zodiak" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="140dp"
        android:layout_height="27dp"
        android:layout_marginEnd="30dp"
        android:text="Cari Shio"
        android:textAlignment="center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ramalan" />

</androidx.constraintlayout.widget.ConstraintLayout>